      <bpmn:outgoing>Flow_0cqrhmw</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:association id="Association_10emj7d" associationDirection="One" sourceRef="Event_0jtn4c1" targetRef="RemoveUserFromHRSystemTask" />
